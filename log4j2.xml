<?xml version="1.0" encoding="UTF-8"?>
<Configuration packages="com.func_vehicle.gtablock">
  <Appenders>
    <JTextAreaAppender name="TextArea">
      <PatternLayout pattern="%d{HH:mm:ss} [%-5level] %logger{36}- %msg%n"/>
    </JTextAreaAppender>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="TextArea"/>
    </Root>
  </Loggers>
</Configuration>